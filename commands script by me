
local screengui = Instance.new("ScreenGui")
local textbox = Instance.new("TextBox")
local textbox2 = Instance.new("TextBox")
screengui.Name = "screengui"
screengui.Parent = game.Players.LocalPlayer.PlayerGui
screengui.ResetOnSpawn = false
textbox.Name = "textbox"
textbox.Parent = screengui
textbox.Position = UDim2.new(0, 800 , 0, 650)
textbox.Size = UDim2.new(0, 850 , 0, 150)
textbox2.Name = "textbox2"
textbox2.Parent = screengui
textbox2.Position = UDim2.new(0, 800 , 0, 800)
textbox2.Size = UDim2.new(0, 850 , 0, 150)
function lttp()
while true do
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot())
wait(0.1)
end
if textbox2.Text == "o" then
return 
end
end
function isj()
while true do
if game.Players.LocalPlayer.Character.Humanoid.Sit == true then
game.Players.LocalPlayer.Character.Humanoid:ChangeState('Jumping')
end
wait(0.1)
end
end
local UserInputService = game.UserInputService
UserInputService.InputBegan:Connect(function(input, gameProcessed)
if input.KeyCode == Enum.KeyCode.Semicolon then
textbox:CaptureFocus()
end
end)
function lpmk()
while true do
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot() + Vector3.new(1, 0, 0))
wait(0.1)
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot() + Vector3.new(-1, 0, 0))
wait(0.1)
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot() + Vector3.new(0, 1, 0))
wait(0.1)
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot() + Vector3.new(0, -1, 0))
wait(0.1)
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot() + Vector3.new(0, 0, 1))
wait(0.1)
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot() + Vector3.new(1, 0, -1))
wait(0.1)
end
end
textbox.FocusLost:Connect(function(enterPressed)
if enterPressed then 
local inputText = textbox.Text
local inputText2 = textbox2.Text
if inputText == ";ws" then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = inputText2
end
if inputText == ";hh" then
game.Players.LocalPlayer.Character.Humanoid.HipHeight = inputText2
end
if inputText == ";ad" then
game.Players.LocalPlayer.Character.Animate:Destroy()
end
if inputText == ";rc" then
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
if inputText == ";st" then
game.Players.LocalPlayer.Character.Humanoid.Sit = true
end
if inputText == ";j" then
game.Players.LocalPlayer.Character.Humanoid:ChangeState('Jumping')
end
if inputText == ";fda" then
game.Players.LocalPlayer.Character.Humanoid:ChangeState('FallingDown')
end
if inputText == ";cf" then
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
end
if inputText == ";ucf" then
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
end
if inputText == ";pfst" then
game.Players.LocalPlayer.Character.Humanoid.PlatformStand = true
end
if inputText == ";pfsf" then
game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
end
if inputText == ";cd" then
workspace.Camera:Destroy()
end
if inputText == ";tpl" then
game.Players.LocalPlayer.Character:PivotTo(game.Players[inputText2].Character:GetPivot())
end
if inputText == ";art" then
game.Players.LocalPlayer.Character.Humanoid.AutoRotate = true
end
if inputText == ";arf" then
game.Players.LocalPlayer.Character.Humanoid.AutoRotate = false
end
if inputText == ";ka" then
for _, a in pairs(game.Players:GetChildren()) do
    a.Character.Humanoid.Health = 0
end
end
if inputText == ";pawo" then
for _, a in pairs(workspace:GetChildren()) do
print(a)
end
end
if inputText == ";xv3" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(inputText2, 0, 0)
end
if inputText == ";yv3" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, inputText2, 0)
end
if inputText == ";zv3" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, 0, inputText2)
end
if inputText == ";lttp" then
lttp()
end
if inputText == ";ps" then
print(game.Players.LocalPlayer.Character.Humanoid.Sit)
end
if inputText == ";isj" then
isj()
end
if inputText == ";fd" then
local pos = game.Players.LocalPlayer.Character:GetPivot()
game.Players.LocalPlayer.Character:PivotTo(pos + Vector3.new(0, 50, 0))
game.Players.LocalPlayer.Character.Humanoid:ChangeState('FallingDown')
wait(0.4)
game.Players.LocalPlayer.Character:PivotTo(pos)
end
if inputText == ";aits" then
local Player = game.Players.LocalPlayer.Name
for _, a in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
a.Parent = workspace[Player]
end
end
if inputText == ";dttm" then
for _, a in pairs(workspace:GetDescendants()) do
if a:IsA("Tool") then
game.Players.LocalPlayer.Character:PivotTo(a:GetPivot())
end
end
end
if inputText == ";mtat" then
for _, a in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
a.Handle.Anchored = true
end
end
if inputText == ";xcamznc" then
local pos = game.Players.LocalPlayer.Character:GetPivot()
game.Players.LocalPlayer.Character:PivotTo(pos + Vector3.new(inputText2, 0, 0))
wait(1)
game.Players.LocalPlayer.Character:PivotTo(pos)
end
if inputText == ";ycamznc" then
local pos = game.Players.LocalPlayer.Character:GetPivot()
game.Players.LocalPlayer.Character:PivotTo(pos + Vector3.new(0, inputText2, 0))
wait(1)
game.Players.LocalPlayer.Character:PivotTo(pos)
end
if inputText == ";zcamznc" then
local pos = game.Players.LocalPlayer.Character:GetPivot()
game.Players.LocalPlayer.Character:PivotTo(pos + Vector3.new(0, 0, inputText2))
wait(1)
game.Players.LocalPlayer.Character:PivotTo(pos)
end
if inputText == ";lpmk" then
lpmk()
end
if inputText == ";coxv3" then
game.Players.LocalPlayer.Character.Humanoid.CameraOffset = Vector3.new(textbox2.Text, 0, 0)
end
if inputText == ";coyv3" then
game.Players.LocalPlayer.Character.Humanoid.CameraOffset = Vector3.new(0, textbox2.Text, 0)
end
if inputText == ";cozv3" then
game.Players.LocalPlayer.Character.Humanoid.CameraOffset = Vector3.new(0, 0, textbox2.Text)
end
if inputText == ";pcp" then
print(game.Players[inputText2].Character:GetPivot())
end
if inputText == ";ttsl" then
game.Players.LocalPlayer.Character:PivotTo(workspace.SpawnLocation.CFrame)
end
if inputText == ";tts" then
for _, f in pairs(workspace:GetDescendants()) do
if f:IsA("Seat") then
game.Players.LocalPlayer.Character:PivotTo(f.CFrame)
end
end
end
if inputText == ";ptaw1mpm" then
local p = game.Players.LocalPlayer.Character:GetPivot()
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot())
wait(1)
game.Players.LocalPlayer.Character:PivotTo(p)
end
if inputText == ";jad" then
game.Players.LocalPlayer.Character.Humanoid:ChangeState('Jumping')
wait(0.1)
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
if inputText == ";tpad" then
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot())
wait(0.1)
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
if inputText == ";xvad" then
game.Players.LocalPlayer.Character:PivotTo(game.Players.LocalPlayer.Character:GetPivot() + Vector3.new(textbox2.Text, 0, 0))
wait(0.1)
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
if inputText == ";yvad" then
game.Players.LocalPlayer.Character:PivotTo(game.Players.LocalPlayer.Character:GetPivot() + Vector3.new(0, textbox2.Text, 0))
wait(0.1)
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
if inputText == ";zvad" then
game.Players.LocalPlayer.Character:PivotTo(game.Players.LocalPlayer.Character:GetPivot() + Vector3.new(0, 0, textbox2.Text))
wait(0.1)
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
if inputText == ";ttxv3apc" then
game.Players.LocalPlayer.Character:PivotTo(game.Players.LocalPlayer.Character:GetPivot() + Vector3.new(textbox2.Text, 0, 0))
wait(0.1)
print(game.Players.LocalPlayer.Character:GetPivot())
end
if inputText == ";ttyv3apc" then
game.Players.LocalPlayer.Character:PivotTo(game.Players.LocalPlayer.Character:GetPivot() + Vector3.new(0, textbox2.Text, 0))
wait(0.1)
print(game.Players.LocalPlayer.Character:GetPivot())
end
if inputText == ";ttzv3apc" then
game.Players.LocalPlayer.Character:PivotTo(game.Players.LocalPlayer.Character:GetPivot() + Vector3.new(0, 0, textbox2.Text))
wait(0.1)
print(game.Players.LocalPlayer.Character:GetPivot())
end
if inputText == ";ttpapc" then
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot())
wait(0.1)
print(game.Players.LocalPlayer.Character:GetPivot())
end
if inputText == ";gd" then
for _, a in pairs(game.Players.LocalPlayer.PlayerGui:GetChildren()) do
a:Destroy()
end
end
if inputText == ";ttuap" then
for _, a in pairs(workspace:GetDescendants()) do
if a:IsA("Part") then
if a.Anchored == false then
game.Players.LocalPlayer.Character:PivotTo(a.CFrame)
end
end
end
end
if inputText == ";sgd" then
game.Players.LocalPlayer.PlayerGui.screengui:Destroy()
end
if inputText == ";pauap" then
for _, a in pairs(workspace:GetDescendants()) do
if a:IsA("Part") then
if a.Anchored == false then
print(a)
end
end
end
end
if inputText == ";paitp" then
for _, a in pairs(game.Players[textbox2.Text].Backpack:GetChildren()) do
print(a)
end
for _, a in pairs(workspace[textbox2.Text]:GetChildren()) do
if a:IsA("Tool") then
print(a)
end
end
end
if inputText == ";tsatpaw1mbt" then
local d = game.Players.LocalPlayer.Name
for _, a in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
a.Parent = workspace[d]
end
local s = game.Players.LocalPlayer.Character:GetPivot()
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot())
wait(1)
game.Players.LocalPlayer.Character:PivotTo(s)
end
if inputText == ";sad" then
game.Players.LocalPlayer.Character.Humanoid.Sit = true
wait(0.1)
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
if inputText == ";fdad" then
local pos = game.Players.LocalPlayer.Character:GetPivot()
game.Players.LocalPlayer.Character:PivotTo(pos + Vector3.new(0, 50, 0))
game.Players.LocalPlayer.Character.Humanoid:ChangeState('FallingDown')
wait(0.1)
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
if inputText == ";ttsl" then
for _, a in pairs(workspace:GetDescendants()) do
if a.Name == "SpawnLocation" then
game.Players.LocalPlayer.Character:PivotTo(a.CFrame)
end
end
end
if inputText == ";ttslacf" then
for _, a in pairs(workspace:GetDescendants()) do
if a.Name == "SpawnLocation" then
game.Players.LocalPlayer.Character:PivotTo(a.CFrame)
wait(0.1)
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
end
end
end
if inputText == ";ttslad" then
for _, a in pairs(workspace:GetDescendants()) do
if a.Name == "SpawnLocation" then
game.Players.LocalPlayer.Character:PivotTo(a.CFrame)
wait(0.1)
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
end
end
if inputText == ";ttslapc" then
for _, a in pairs(workspace:GetDescendants()) do
if a.Name == "SpawnLocation" then
game.Players.LocalPlayer.Character:PivotTo(a.CFrame)
wait(0.1)
print(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame)
end
end
end
if inputText == ";cratxv3" then
game.Players.LocalPlayer.Character.Humanoid.Health = 0
wait(0.1)
game.Players.LocalPlayer.Character:PivotTo(game.Players.LocalPlayer.Character:GetPivot() + Vector3.new(textbox2.Text, 0, 0))
end
if inputText == ";cratyv3" then
game.Players.LocalPlayer.Character.Humanoid.Health = 0
wait(0.1)
game.Players.LocalPlayer.Character:PivotTo(game.Players.LocalPlayer.Character:GetPivot() + Vector3.new(0, textbox2.Text, 0))
end
if inputText == ";cratzv3" then
game.Players.LocalPlayer.Character.Humanoid.Health = 0
wait(0.1)
game.Players.LocalPlayer.Character:PivotTo(game.Players.LocalPlayer.Character:GetPivot() + Vector3.new(0, 0, textbox2.Text))
end
end
end)
