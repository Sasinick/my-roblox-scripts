local p = "beekon1066"
local sasinickgui = Instance.new("ScreenGui")
local frame = Instance.new("Frame")
local uidraggingdetector = Instance.new("UIDragDetector")
local textlabel = Instance.new("TextLabel")
local textbutton1 = Instance.new("TextButton")
local textbutton2 = Instance.new("TextButton")
local textbutton3 = Instance.new("TextButton")
local textbutton4 = Instance.new("TextButton")
local textbutton5 = Instance.new("TextButton")
local textbutton6 = Instance.new("TextButton")
local textbutton7 = Instance.new("TextButton")
local textbutton8 = Instance.new("TextButton")
local textbutton9 = Instance.new("TextButton")
local textbutton10 = Instance.new("TextButton")
local textbutton11 = Instance.new("TextButton")
local textbutton12 = Instance.new("TextButton")
local textbutton13 = Instance.new("TextButton")
local textbutton14 = Instance.new("TextButton")
local textbutton15 = Instance.new("TextButton")
local textbutton16 = Instance.new("TextButton")
sasinickgui.Name = "sasinickgui"
sasinickgui.Parent = game.Players[p].PlayerGui
sasinickgui.ResetOnSpawn = false
frame.Parent = sasinickgui
frame.Size = UDim2.new(0.2, 0.2, 0.2)
uidraggingdetector.Parent = frame
textlabel.Parent = frame
textlabel.Size = UDim2.new(0.2, 0.2, 0.2)
textlabel.Text = "SasinickGui"
textlabel.Position = UDim2.new(0.004, 0.1, 0.01)
textbutton1.Parent = frame
textbutton1.Text = "kill all npcs and players"
textbutton1.Position = UDim2.new(0.004, 0.1, 0.15)
textbutton1.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton1.MouseButton1Click:Connect(function()
for _, a in pairs(workspace:GetDescendants()) do
if a:IsA("Model") then
if a:FindFirstChild("Humanoid") then
a.Humanoid.Health = 0
end
end
end
end)
textbutton2.Parent = frame
textbutton2.Text = "bring all npcs and players"
textbutton2.Position = UDim2.new(0.004, 0.1, 0.27)
textbutton2.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton2.MouseButton1Click:Connect(function()
local name = p
local playerposition = game.Players[name].Character.HumanoidRootPart.CFrame
for _, a in pairs(workspace:GetDescendants()) do
if a:IsA("Model") then
if a:FindFirstChild("HumanoidRootPart") then
a.HumanoidRootPart.CFrame = playerposition
end
end
end
end)
textbutton3.Parent = frame
textbutton3.Text = "all players walkspeed change to 0"
textbutton3.Position = UDim2.new(0.004, 0.1, 0.39)
textbutton3.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton3.MouseButton1Click:Connect(function()
for _, a in pairs(game.Players:GetPlayers()) do
a.Character.Humanoid.WalkSpeed = 0
end
end)
textbutton4.Parent = frame
textbutton4.Text = "part rain"
textbutton4.Position = UDim2.new(0.004, 0.1, 0.52)
textbutton4.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton4.MouseButton1Click:Connect(function()
local name = p
local positionY = game.Players[name].Character.HumanoidRootPart.CFrame.Y + 30
while true do
local a = math.random(-1000, 1000)
local b = math.random(-1000, 1000)
local part = Instance.new("Part")
part.Anchored = false
part.Parent = workspace
part.CFrame = CFrame.new(a, positionY, b)
task.wait(0.1)
end
while true do
local a = math.random(-1000, 1000)
local b = math.random(-1000, 1000)
local part = Instance.new("Part")
part.Anchored = false
part.Parent = workspace
part.CFrame = CFrame.new(a, positionY, b)
task.wait(0.1)
end
while true do
local a = math.random(-1000, 1000)
local b = math.random(-1000, 1000)
local part = Instance.new("Part")
part.Anchored = false
part.Parent = workspace
part.CFrame = CFrame.new(a, positionY, b)
task.wait(0.1)
end
end)
textbutton5.Parent = frame
textbutton5.Text = "all players guis delete"
textbutton5.Position = UDim2.new(0.004, 0.1, 0.52)
textbutton5.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton5.MouseButton1Click:Connect(function()
for _, a in pairs(game.Players:GetPlayers()) do
if a:FindFirstChild("PlayerGui") then
a.PlayerGui:Destroy()
end
end
end)
textbutton6.Parent = frame
textbutton6.Text = "loop all players guis delete"
textbutton6.Position = UDim2.new(0.004, 0.1, 0.64)
textbutton6.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton6.MouseButton1Click:Connect(function()
for _, a in pairs(game.Players:GetPlayers()) do
if a:FindFirstChild("PlayerGui") then
a.PlayerGui:Destroy()
end
end
game.Players.PlayerAdded:Connect(function(player)
task.wait(1)
for _, a in pairs(game.Players:GetPlayers()) do
if a:FindFirstChild("PlayerGui") then
a.PlayerGui:Destroy()
end
end
end)
end)
textbutton7.Parent = frame
textbutton7.Text = "all parts color random"
textbutton7.Position = UDim2.new(0.004, 0.1, 0.76)
textbutton7.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton7.MouseButton1Click:Connect(function()
for _, part in pairs(workspace:GetDescendants()) do
if part:IsA("BasePart") then
local color1 = math.random()
local color2 = math.random()
local color3 = math.random()
part.Color = Color3.new(color1, color2, color3)
end
end
end)
textbutton8.Parent = frame
textbutton8.Text = "loop all parts color random"
textbutton8.Position = UDim2.new(0.004, 0.1, 0.88)
textbutton8.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton8.MouseButton1Click:Connect(function()
while true do
for _, part in pairs(workspace:GetDescendants()) do
if part:IsA("BasePart") then
local color1 = math.random()
local color2 = math.random()
local color3 = math.random()
part.Color = Color3.new(color1, color2, color3)
end
end
task.wait(0.1)
end
end)
textbutton9.Parent = frame
textbutton9.Text = "kick all players"
textbutton9.Position = UDim2.new(0.25, 0.01, 0.01)
textbutton9.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton9.MouseButton1Click:Connect(function()
for _, a in pairs(game.Players:GetPlayers()) do
a:Destroy()
end
end)
textbutton10.Parent = frame
