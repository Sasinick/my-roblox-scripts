textbutton12.Text = "part rain"
textbutton12.Position = UDim2.new(0.88, 0.01, 0.15)
textbutton12.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton12.MouseButton1Click:Connect(function()
local name = p
local positionY = game.Players[name].Character.HumanoidRootPart.CFrame.Y + 30
while true do
local a = math.random(-500, 500)
local b = math.random(-500, 500)
local part = Instance.new("Part")
part.Anchored = false
