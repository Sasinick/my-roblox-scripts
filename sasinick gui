local pos = nil
local respawntime = game.Players.RespawnTime
local sasinickgui = Instance.new("ScreenGui")
local frame = Instance.new("Frame")
local uidraggingdetector = Instance.new("UIDragDetector")
local textlabel = Instance.new("TextLabel")
local textbutton1 = Instance.new("TextBox")
local textbutton2 = Instance.new("TextBox")
local textbutton3 = Instance.new("TextButton")
local textbutton4 = Instance.new("TextBox")
local textbutton5 = Instance.new("TextButton")
local textbutton6 = Instance.new("TextButton")
local textbutton7 = Instance.new("TextBox")
local textbutton8 = Instance.new("TextButton")
local textbutton9 = Instance.new("TextButton")
local textbutton10 = Instance.new("TextButton")
local textbutton11 = Instance.new("TextButton")
local textbutton12 = Instance.new("TextBox")
local textbutton13 = Instance.new("TextBox")
local textbutton14 = Instance.new("TextBox")
local textbutton15 = Instance.new("TextButton")
local textbutton16 = Instance.new("TextButton")
local textbutton17 = Instance.new("TextButton")
local textbutton18 = Instance.new("TextButton")
local textbutton19 = Instance.new("TextButton")
local textbutton20 = Instance.new("TextButton")
sasinickgui.Name = "sasinickgui"
sasinickgui.Parent = game.Players.LocalPlayer.PlayerGui
sasinickgui.ResetOnSpawn = false
frame.Parent = sasinickgui
frame.Size = UDim2.new(0.2, 0.2, 0.2)
uidraggingdetector.Parent = frame
textlabel.Parent = frame
textlabel.Size = UDim2.new(0.2, 0.2, 0.2)
textlabel.Text = "SasinickGui"
textlabel.Position = UDim2.new(0.004, 0.1, 0.01)
textbutton1.Parent = frame
textbutton1.Text = "walkspeed"
textbutton1.Position = UDim2.new(0.004, 0.1, 0.15)
textbutton1.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton1.FocusLost:Connect(function()
function changewalkspeed()
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = textbutton1.Text
end
changewalkspeed()
end)
textbutton2.Parent = frame
textbutton2.Text = "jumppower"
textbutton2.Position = UDim2.new(0.004, 0.1, 0.27)
textbutton2.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton2.FocusLost:Connect(function()
game.Players.LocalPlayer.Character.Humanoid.UseJumpPower = true
function changejumppower()
game.Players.LocalPlayer.Character.Humanoid.JumpPower = textbutton2.Text
end
changejumppower()
end)
textbutton3.Parent = frame
textbutton3.Text = "jump"
textbutton3.Position = UDim2.new(0.004, 0.1, 0.39)
textbutton3.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton3.MouseButton1Click:Connect(function()
game.Players.LocalPlayer.Character.Humanoid:ChangeState('Jumping')
end)
textbutton4.Parent = frame
textbutton4.Text = "hipheight"
textbutton4.Position = UDim2.new(0.004, 0.1, 0.56)
textbutton4.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton4.FocusLost:Connect(function()
game.Players.LocalPlayer.Character.Humanoid.HipHeight = textbutton4.Text
end)
textbutton7.Parent = frame
textbutton7.Text = "transparency"
textbutton7.Position = UDim2.new(0.004, 0.1, 0.76)
textbutton7.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton7.FocusLost:Connect(function()
for _, a in pairs(workspace:GetDescendants()) do
if a:IsA("BasePart") then
a.Transparency = textbutton7.Text
end
end
end)
textbutton8.Parent = frame
textbutton8.Text = "click to teleport tool"
textbutton8.Position = UDim2.new(0.004, 0.1, 0.88)
textbutton8.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton8.MouseButton1Click:Connect(function()
function createtool()
local tool = Instance.new("Tool")
tool.Name = "click to teleport tool"
tool.RequiresHandle = false
tool.CanBeDropped = false
tool.Parent = game.Players.LocalPlayer.Backpack
tool.Activated:Connect(function()
local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
local targetpos = mouse.Hit.p
player.Character:MoveTo(targetpos)
end)
end
createtool()
end)
textbutton9.Parent = frame
textbutton9.Text = "noclip and anti fling"
textbutton9.Position = UDim2.new(0.25, 0.01, 0.01)
textbutton9.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton9.MouseButton1Click:Connect(function()
for _, a in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
if a:IsA("BasePart") then
a.CanCollide = false
end
end
end)
textbutton10.Parent = frame
textbutton10.Text = "save pos"
textbutton10.Position = UDim2.new(0.46, 0.01, 0.01)
