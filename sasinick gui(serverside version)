textbutton15.MouseButton1Click:Connect(function()
local name = p
while true do
if game.Players[name] and game.Players[name].Character.HumanoidRootPart then
local uapart = Instance.new("Part")
uapart.Parent = workspace
uapart.Anchored = true
uapart.CFrame = game.Players[name].Character.HumanoidRootPart.CFrame
end
task.wait(0.1)
end
end)
textbutton16.Parent = frame
