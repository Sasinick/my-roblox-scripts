for _, a in pairs(workspace.Map.BaseGround:GetDescendants()) do
if a:IsA("Part") then
a.CanCollide = false
end
end
wait(0.1)
local part = Instance.new("Part")
part.Name = "djis"
part.Anchored = true
part.CFrame = workspace.Map.BaseGround.CFrame + Vector3.new(0, 30, 0)
part.Size = Vector3.new(900, 1, 900)
part.Parent = workspace
