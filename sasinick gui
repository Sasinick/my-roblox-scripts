textbutton30.Parent = frame
textbutton30.Text = "get drop tools"
textbutton30.Position = UDim2.new(0.48, 0.01, 0.84)
textbutton30.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton30.MouseButton1Click:Connect(function()
	for _, droptools in pairs(workspace:GetDescendants()) do
		if droptools:IsA("Tool") then
			if droptools:FindFirstChild("Handle") then
				if droptools.Parent == workspace then
					local droptoolsposition = droptools.Handle.CFrame
					droptools.Handle.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
				end
			end
		end
	end
end)
