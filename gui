textbutton11.Text = "character reset"
textbutton11.Position = UDim2.new(0.30, 0, 0.09)
textbutton11.MouseButton1Click:Connect(function()
local d = game.Players.LocalPlayer.Character:GetPivot()
local s = game.Players.RespawnTime
game.Players.LocalPlayer.Character.Humanoid.Health = 0
wait(s + 0.5)
game.Players.LocalPlayer.Character:PivotTo(d)
end)
