textbutton8.Parent = frame
textbutton8.Text = "click to teleport tool"
textbutton8.Position = UDim2.new(0.004, 0.1, 0.88)
textbutton8.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton8.MouseButton1Click:Connect(function()
	function createtool()
		local tool = Instance.new("Tool")
		tool.Name = "click to teleport tool"
		tool.RequiresHandle = false
		tool.CanBeDropped = false
		tool.Parent = game.Players.LocalPlayer.Backpack
		tool.Activated:Connect(function()
			local player = game.Players.LocalPlayer
			local mouse = player:GetMouse()
			local targetpos = mouse.Hit.p
			player.Character:MoveTo(targetpos)
		end)
	end
	createtool()
end)
