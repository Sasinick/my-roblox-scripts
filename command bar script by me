local screengui = Instance.new("ScreenGui")
local textbox = Instance.new("TextBox")
screengui.Parent = game.Players.LocalPlayer.PlayerGui
textbox.Parent = screengui
textbox.Position = UDim2.new(0, 800 , 0, 8)
textbox.Size = UDim2.new(0, 200 , 0, 50) 
textbox.FocusLost:Connect(function(enterPressed)
if enterPressed then 
local inputText = textbox.Text
if inputText == "wsct180" then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 180
end
if inputText == "wsct16" then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end
if inputText == "hhct50" then
game.Players.LocalPlayer.Character.Humanoid.HipHeight = 50
end
if inputText == "hhct10" then
game.Players.LocalPlayer.Character.Humanoid.HipHeight = 10
end
if inputText == "hhct0" then
game.Players.LocalPlayer.Character.Humanoid.HipHeight = 0
end
if inputText == "hhct3" then
game.Players.LocalPlayer.Character.Humanoid.HipHeight = 3
end
if inputText == "st" then
game.Players.LocalPlayer.Character.Humanoid.Sit = true
end
if inputText == "cr" then
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
if inputText == "j" then
game.Players.LocalPlayer.Character.Humanoid:ChangeState('Jumping')
end
if inputText == "ad" then
game.Players.LocalPlayer.Character.Animate:Destroy()
end
if inputText == "sf" then
game.Players.LocalPlayer.Character.Humanoid.Sit = false
end
if inputText == "art" then
game.Players.LocalPlayer.Character.Humanoid.AutoRotate = true
end
if inputText == "arf" then
game.Players.LocalPlayer.Character.Humanoid.AutoRotate = false
end
if inputText == "cd" then
workspace.Camera:Destroy()
end
if inputText == "cf" then
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
end
if inputText == "ucf" then
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
end
if inputText == "awoo" then
for _, a in pairs(workspace:GetChildren()) do
print(a)
end
end
if inputText == "hd" then
game.Players.LocalPlayer.Character.Humanoid:Destroy()
end
end
end)
