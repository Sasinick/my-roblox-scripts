textbutton23.Text = textbutton23text
textbutton23.Position = UDim2.new(0.69, 0.01, 0.38)
textbutton23.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton23.MouseButton1Click:Connect(function()
	for _, tools in pairs(game.Players.LocalPlayer.Backpack:GetDescendants()) do
		if tools:IsA("Tool") and tools.CanBeDropped ~= false then
			tools.Parent = workspace[playername]
		end
	end
	for _, tools in pairs(game.Players.LocalPlayer.Character:GetDescendants()) do
		if tools:IsA("Tool") and tools.CanBeDropped ~= false then
			tools.Parent = workspace
		end
	end
end)
