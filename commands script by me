local screengui = Instance.new("ScreenGui")
local textbox = Instance.new("TextBox")
local textbox2 = Instance.new("TextBox")
screengui.Name = "screengui"
screengui.Parent = game.Players.LocalPlayer.PlayerGui
textbox.Name = "textbox"
textbox.Parent = screengui
textbox.Position = UDim2.new(0, 800 , 0, 650)
textbox.Size = UDim2.new(0, 850 , 0, 150)
textbox2.Name = "textbox2"
textbox2.Parent = screengui
textbox2.Position = UDim2.new(0, 800 , 0, 800)
textbox2.Size = UDim2.new(0, 850 , 0, 150)
function lttp()
while true do
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot())
wait(0.1)
end
if textbox2.Text == "o" then
return 
end
end
function isj()
while true do
if game.Players.LocalPlayer.Character.Humanoid.Sit == true then
game.Players.LocalPlayer.Character.Humanoid:ChangeState('Jumping')
end
wait(0.1)
end
end
local UserInputService = game.UserInputService
UserInputService.InputBegan:Connect(function(input, gameProcessed)
if input.KeyCode == Enum.KeyCode.R then
textbox:CaptureFocus()
end
end)
function lpmk()
while true do
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot() + Vector3.new(1, 0, 0))
wait(0.1)
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot() + Vector3.new(-1, 0, 0))
wait(0.1)
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot() + Vector3.new(0, 5, 0))
wait(0.1)
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot() + Vector3.new(0, -5, 0))
wait(0.1)
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot() + Vector3.new(0, 0, 1))
wait(0.1)
game.Players.LocalPlayer.Character:PivotTo(game.Players[textbox2.Text].Character:GetPivot() + Vector3.new(1, 0, -1))
wait(0.1)
end
end
textbox.FocusLost:Connect(function(enterPressed)
if enterPressed then 
local inputText = textbox.Text
local inputText2 = textbox2.Text
if inputText == "rws" then
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = inputText2
end
if inputText == "rhh" then
game.Players.LocalPlayer.Character.Humanoid.HipHeight = inputText2
end
if inputText == "rad" then
game.Players.LocalPlayer.Character.Animate:Destroy()
end
if inputText == "rcr" then
game.Players.LocalPlayer.Character.Humanoid.Health = 0
end
if inputText == "rst" then
game.Players.LocalPlayer.Character.Humanoid.Sit = true
end
if inputText == "rj" then
game.Players.LocalPlayer.Character.Humanoid:ChangeState('Jumping')
end
if inputText == "rfda" then
game.Players.LocalPlayer.Character.Humanoid:ChangeState('FallingDown')
end
if inputText == "rcf" then
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = true
end
if inputText == "rucf" then
game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored = false
end
if inputText == "rpfst" then
game.Players.LocalPlayer.Character.Humanoid.PlatformStand = true
end
if inputText == "rpfsf" then
game.Players.LocalPlayer.Character.Humanoid.PlatformStand = false
end
if inputText == "rcd" then
workspace.Camera:Destroy()
end
if inputText == "rtpl" then
game.Players.LocalPlayer.Character:PivotTo(game.Players[inputText2].Character:GetPivot())
end
if inputText == "rart" then
game.Players.LocalPlayer.Character.Humanoid.AutoRotate = true
end
if inputText == "rarf" then
game.Players.LocalPlayer.Character.Humanoid.AutoRotate = false
end
if inputText == "rka" then
for _, a in pairs(game.Players:GetChildren()) do
    a.Character.Humanoid.Health = 0
end
end
if inputText == "rpawo" then
for _, a in pairs(workspace:GetChildren()) do
print(a)
end
end
if inputText == "rxv3" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(inputText2, 0, 0)
end
if inputText == "ryv3" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, inputText2, 0)
end
if inputText == "rzv3" then
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame + Vector3.new(0, 0, inputText2)
end
if inputText == "rlttp" then
lttp()
end
if inputText == "rps" then
print(game.Players.LocalPlayer.Character.Humanoid.Sit)
end
if inputText == "risj" then
isj()
end
if inputText == "rfd" then
local pos = game.Players.LocalPlayer.Character:GetPivot()
game.Players.LocalPlayer.Character:PivotTo(pos + Vector3.new(0, 50, 0))
game.Players.LocalPlayer.Character.Humanoid:ChangeState('FallingDown')
wait(0.4)
game.Players.LocalPlayer.Character:PivotTo(pos)
end
if inputText == "raits" then
local Player = game.Players.LocalPlayer.Name
for _, a in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
a.Parent = workspace[Player]
end
end
if inputText == "rdttm" then
for _, a in pairs(workspace:GetChildren()) do
if a:IsA("Tool") then
game.Players.LocalPlayer.Character:PivotTo(a:GetPivot())
end
end
end
if inputText == "rmtat" then
for _, a in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
a.Handle.Anchored = true
end
end
if inputText == "rxcamznc" then
local pos = game.Players.LocalPlayer.Character:GetPivot()
game.Players.LocalPlayer.Character:PivotTo(pos + Vector3.new(inputText2, 0, 0))
wait(1)
game.Players.LocalPlayer.Character:PivotTo(pos)
end
if inputText == "rycamznc" then
local pos = game.Players.LocalPlayer.Character:GetPivot()
game.Players.LocalPlayer.Character:PivotTo(pos + Vector3.new(0, inputText2, 0))
wait(1)
game.Players.LocalPlayer.Character:PivotTo(pos)
end
if inputText == "rzcamznc" then
local pos = game.Players.LocalPlayer.Character:GetPivot()
game.Players.LocalPlayer.Character:PivotTo(pos + Vector3.new(0, 0, inputText2))
wait(1)
game.Players.LocalPlayer.Character:PivotTo(pos)
end
if inputText == "rlpmk" then
lpmk()
end
end
end)
