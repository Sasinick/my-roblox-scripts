script.Parent = frame
script.Position = UDim2.new(0, 0, 0)
script.Size = UDim2.new(0.35, 0.35, 0.35)
execute.Name = "execute"
execute.Parent = frame
execute.Position = UDim2.new(1, 1, 1)
execute.Size = UDim2.new(0.35, 0.35, 0.35)
execute.Text = "execute"
execute.MouseButton1Click:Connect(function()
loadstring(script.Text)()
end)
end)
