local tool1 = Instance.new("Tool")
tool1.RequiresHandle = false
tool1.Name = "fire me"
tool1.Activated:connect(function()
local fire = Instance.new("Fire")
fire.Heat = 40
fire.Size = 20
fire.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
end)
tool1.Parent = game.Players.LocalPlayer.Backpack
local tool2 = Instance.new("Tool")
tool2.RequiresHandle = false
tool2.Name = "kill all player"
tool2.Activated:connect(function()
for _, a in pairs(game.Players:GetChildren()) do
    a.Character.Humanoid.Health = 0
end
end)
tool2.Parent = game.Players.LocalPlayer.Backpack
local tool3 = Instance.new("Tool")
tool3.RequiresHandle = false
tool3.Name = "kick all player"
tool3.Activated:connect(function()
for _, a in pairs(game.Players:GetChildren()) do
a:Destroy()
end
end)
tool3.Parent = game.Players.LocalPlayer.Backpack
local tool4 = Instance.new("Tool")
tool4.RequiresHandle = false
tool4.Name = "hint aaaaa"
tool4.Activated:connect(function()
local hint = Instance.new("Hint")
hint.Parent = workspace
hint.Text = "aaaaa"
end)
tool4.Parent = game.Players.LocalPlayer.Backpack
local tool5 = Instance.new("Tool")
tool5.RequiresHandle = false
tool5.Name = "all part cancollide = false"
tool5.Activated:connect(function()
for _, a in pairs(workspace:GetDescendants()) do
if a:IsA("Part") then
a.CanCollide = false
end
end
end)
tool5.Parent = game.Players.LocalPlayer.Backpack
local tool6 = Instance.new("Tool")
tool6.RequiresHandle = false
tool6.Name = "all part cancollide = true"
tool6.Activated:connect(function()
for _, a in pairs(workspace:GetDescendants()) do
if a:IsA("Part") then
a.CanCollide = true
end
end
end)
tool6.Parent = game.Players.LocalPlayer.Backpack
local tool7 = Instance.new("Tool")
tool7.RequiresHandle = false
tool7.Name = "all part anchored = false"
tool7.Activated:connect(function()
for _, a in pairs(workspace:GetDescendants()) do
if a:IsA("Part") then
a.Anchored = false
end
end
end)
tool7.Parent = game.Players.LocalPlayer.Backpack
