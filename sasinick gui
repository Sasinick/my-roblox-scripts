textbutton15.Text = "animation freeze"
textbutton15.Position = UDim2.new(0.88, 0.01, 0.47)
textbutton15.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton15.MouseButton1Click:Connect(function()
game.Players.LocalPlayer.Character.Animate:Destroy()
end)
textbutton16.Parent = frame
textbutton16.Text = "refresh"
textbutton16.Position = UDim2.new(0.88, 0.01, 0.60)
textbutton16.Size = UDim2.new(0.16, 0.16, 0.16)
textbutton16.MouseButton1Click:Connect(function()
local pos2 = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
game.Players.LocalPlayer.Character.Humanoid.Health = 0
task.wait(respawntime + 0.5)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos2
end)
